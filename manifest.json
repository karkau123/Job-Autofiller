{
  "manifest_version": 3,
  "name": "Job Autofiller",
  "version": "1.0",
  "description": "Automatically fill job application forms with your saved information",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": ["form.html", "form.js", "form.css"],
      "matches": ["<all_urls>"]
    }
  ]
}